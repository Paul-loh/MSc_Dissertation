         *  SqlConnection myADONETConnection = (SqlConnection)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Sales DB"].ReleaseConnection(rawConnection);
         *
         * Example of using a File connection manager
         *  object rawConnection = Dts.Connections["Prices.zip"].AcquireConnection(Dts.Transaction);
         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion

        string errors = "";

        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            bool success = true;
            
            DateTime dt;

            try
            {
                foreach (Variable v in Dts.Variables)
                {
                    string s = v.Value.ToString();

                    string n_space = v.Namespace;
                    string v_name = v.Name;
                    string q_name = v.QualifiedName;
                    string v_value = v.Value.ToString();


                    if (v_name.StartsWith("ConnString"))
                    {
                        string conn_str = v_value;

                        if (conn_str != null && conn_str.Trim() != "")
                        {
                            bool connWorks = TestConnectionString(conn_str);

                            if (!connWorks)
                                errors += "Connection Error - Parameter: " + q_name + " - Value: " + conn_str + ". ";

                            success = success && connWorks;
                        }
                        else
                        {
                            errors += "Invalid Connection Parameter: " + q_name + " - Value: " + conn_str + ". ";
                            success = false;
                        }

                    }
                    else if (v_name.StartsWith("FileIn"))
                    {
                        string fileIn = v_value;

                        if (fileIn != null && fileIn.Trim() != "")
                        {
                            bool fileExists = System.IO.File.Exists(fileIn);

                            if (!fileExists)
                                errors += "File Exists Fail - Parameter: " + q_name + " - Value: " + fileIn + ". ";

                            success = success && fileExists;
                        }
                        else
                        {
                            errors += "Invalid Parameter: " + q_name + " - Value: " + fileIn + ". ";
                            success = false;
                        }

                    }
                    else if (v_name.StartsWith("DateTime"))         // Validate Datetime value with milliseconds
                    {

                        string date = v_value;
                        if (date != null && date.Trim() != "")
                        {
                            bool dateParsed = DateTime.TryParseExact(date, "yyyy-MM-dd HH:mm:ss.fff", CultureInfo.InvariantCulture, DateTimeStyles.None, out dt);
                            if (!dateParsed)
                                errors += "Invalid DateTime format - Parameter: " + q_name + " - Value: " + date + ". ";

                            success = success && dateParsed;
                        }

                    }
                    else if (v_name.StartsWith("Date"))
                    {

                        string date = v_value;
                        if (date != null && date.Trim() != "")
                        {
                            bool dateParsed = DateTime.TryParseExact(date, "yyyy-MM-dd", CultureInfo.InvariantCulture, DateTimeStyles.None, out dt);
                            if (!dateParsed)
                                errors += "Invalid Date format - Parameter: " + q_name + " - Value: " + date + ". ";

                            success = success && dateParsed;
                        }

                    }
                    else if (v_name.StartsWith("FileName"))
                    {
                        string fileName = v_value;
                        if (fileName == null || fileName.Trim() == "" || fileName.IndexOfAny(System.IO.Path.GetInvalidFileNameChars()) != -1)
                        {
                            success = false;
                            errors += "Invalid Parameter: " + q_name + " - Value: " + fileName + ". ";

                        }

                    }
                    else if (v_name.StartsWith("FileOut"))
                    {
                        string fileOut = v_value;
                        string directory = new FileInfo(fileOut).Directory.FullName;
                        string fileName = Path.GetFileName(fileOut);

                        if (fileOut == null || fileOut.Trim() == "" || fileName.IndexOfAny(System.IO.Path.GetInvalidFileNameChars()) != -1 || !Directory.Exists(directory))
                        {
                            success = false;
                            errors += "Invalid Parameter: " + q_name + " - Value: " + fileOut + ". ";

                        }

                    }
                    else if (v_name.StartsWith("Path"))
                    {
                        string path = v_value;
                        if (!System.IO.Directory.Exists(path))
                        {
                            success = false;
                            errors += "Invalid Parameter: " + q_name + " - Value: " + path + ". ";
                        }
                    }
                }

            }
            catch (Exception ex)
            {
                errors += ex.Message;
            }

            Dts.Variables["User::ErrorMessage"].Value = errors;

            if (success)
                Dts.TaskResult = (int)ScriptResults.Success;
            else
                Dts.TaskResult = (int)ScriptResults.Failure;

        }

        public bool TestConnectionString(string connString)
        {

            bool works = true;

            try
            {
                OleDbConnection conn = new OleDbConnection(connString);
                conn.Open();
                DataTable dt = conn.GetSchema();
                works = dt.Rows.Count > 0;
                conn.Close();

            } catch (Exception ex) {
                works = false;
                errors += "Connection String Error: " + ex.Message + ". "; 
            }
            return works;
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
